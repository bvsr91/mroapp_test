{
    "_version": "1.40.0",
    "sap.app": {
        "id": "com.ferre.mrouife",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap-ux/fiori-freestyle-writer:worklist",
            "version": "0.11.15",
            "toolsId": "7c438834-277c-4e11-9f3d-3f4c42cc7f95"
        },
        "dataSources": {
            "mainService": {
                "uri": "/mro/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            },
            "mrosrv_v2": {
                "uri": "/v2/mro/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.102.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.ferre.mrouife.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "mrosrv_v2": {
                "dataSource": "mrosrv_v2",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "userModel": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "side": {
              "type": "sap.ui.model.json.JSONModel",
              "uri": "model/sideContent.json"
            },
            "vendorData": {
              "type": "sap.ui.model.json.JSONModel",
              "uri": "model/VendorData.json"
            },
            "pricingData": {
              "type": "sap.ui.model.json.JSONModel",
              "uri": "model/pricingData.json"
            },
            "notificationData": {
              "type": "sap.ui.model.json.JSONModel",
              "uri": "model/notificationData.json"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "com.ferre.mrouife.view",
                "controlId": "mainContents",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                },
                "async": true
            },
            "routes": [
                {
                    "name": "RouteApp",
                    "pattern": "RouteApp",
                    "target": ["TargetApp"]
                  },
                  {
                    "name": "search",
                    "pattern": "",
                    "target": "search"
                  },
                  {
                    "name": "vendorList",
                    "pattern": "vendorList",
                    "target": "vendorList"
                  },
                  {
                    "name": "pricingCond",
                    "pattern": "pricing",
                    "target": "pricingCond"
                  },
                  {
                    "name": "myInbox",
                    "pattern": "myInbox",
                    "target": "myInbox"
                  },{
                    "name": "notFound",
                    "pattern": "notFound",
                    "target": "notFound"
                  }
            ],
            "targets": {
                "TargetApp": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "App",
                    "viewName": "App"
                  },
                  "vendorList": {
                    "viewName": "VendorList",
                    "viewType": "XML",
                    "viewId": "VendorList",
                    "clearControlAggregation": false,
                    "transition": "show"
                  },
                  "pricingCond": {
                    "viewName": "PricingCond",
                    "viewType": "XML",
                    "viewId": "PricingCond",
                    "clearControlAggregation": false,
                    "transition": "show"
                  },
                  "myInbox": {
                    "viewName": "MyInbox",
                    "viewType": "XML",
                    "viewId": "MyInbox",
                    "clearControlAggregation": false,
                    "transition": "show"
                  },
                  "search": {
                    "viewName": "Search",
                    "viewType": "XML",
                    "viewId": "Search",
                    "clearControlAggregation": false,
                    "transition": "show"
                  },
                  "notFound": {
                    "viewName": "NotFound",
                    "viewType": "XML",
                    "viewId": "NotFound",
                    "clearControlAggregation": false,
                    "transition": "show"
                  }
            }
        },
        "rootView": {
            "viewName": "com.ferre.mrouife.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        }
    }
}